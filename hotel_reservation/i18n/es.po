"                    <br/>\n"
"                    % endif\n"
"                    <br/>\n"
"                    % if object.warehouse_id.partner_id.website:\n"
"                    <strong>${object.warehouse_id.partner_id.website}</strong>\n"
"                    <br/>\n"
"                    % endif\n"
"                </p>\n"
"            </div>\n"
"        "
