"                    % endif\n"
"                    % if object.warehouse_id.partner_id.country_id.name:\n"
"                    <strong>\n"
"                        ${object.warehouse_id.partner_id.country_id.name}</strong>\n"
"                    ,\n"
"                    <br/>\n"
"                    % endif\n"
"                    <br/>\n"
