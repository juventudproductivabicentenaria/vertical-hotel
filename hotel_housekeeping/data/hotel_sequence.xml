<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="seq_hotel_housekeeping" model="ir.sequence">
        <field name="name">Hotel Housekeeping</field>
        <field name="code">hotel.housekeeping</field>
        <field name="prefix">RA-%(year)s/</field>
        <field name="padding">5</field>
    </record>

    <record id="seq_hotel_activity" model="ir.sequence">
        <field name="name">Hotel Activity</field>
        <field name="code">hotel.housekeeping.activities</field>
        <field name="prefix">HA-%(year)s/</field>
        <field name="padding">5</field>
    </record>
</odoo>
